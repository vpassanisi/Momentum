{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm-run-all -p serve:css serve:js",
    "build": "npm-run-all build:css build:js",
    "serve:js": "vue-cli-service serve --port 5000",
    "build:js": "vue-cli-service build",
    "serve:css": "postcss src/css/tailwind.src.css -o src/css/tailwind.css -w",
    "build:css": "postcss src/css/tailwind.src.css -o src/css/tailwind.css --env production",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@types/prosemirror-inputrules": "^1.0.2",
    "@types/prosemirror-model": "^1.7.2",
    "@types/prosemirror-view": "^1.15.0",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "autoprefixer": "^9.8.6",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.6.2",
    "tailwindcss-dark-mode": "^1.1.6",
    "tiptap": "^1.29.4",
    "tiptap-extensions": "^1.32.1",
    "typescript": "~3.9.3",
    "vue-mq": "^1.0.1",
    "vue-template-compiler": "^2.6.11"
  }
}
